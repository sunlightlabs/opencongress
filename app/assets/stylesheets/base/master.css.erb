~ Pagination

///////////////////////////////////////////////////////*/

.pagination{
  height: 20px;
  float: none;
  margin-bottom: 10px;
  padding: 10px 0px 8px;
  }

.pagination a,
.pagination .previous_page,
.pagination em,
.pagination .gap{
  position: relative;
  display: block;
  float: left;
  font-size: 11px;
  padding: 2px 2px 2px 8px;
  margin-right: 8px;
  background: url('<%= asset_path('small_button.png') %>') repeat left top;
  _background: url('<%= asset_path('small_button.gif') %>') repeat left top;
  text-decoration: none;
  color: #555;
  }

.pagination a:after,
.pagination .previous_page:after,
.pagination em:after{
  content: "";
  position: absolute;
  top: 0;
  padding: 22px 0 0 8px;
  background: url('<%= asset_path('small_button_cap.png') %>') right 0 no-repeat;
  _background: url('<%= asset_path('small_button_cap.gif') %>') right 0 no-repeat;
}

.pagination .gap{
  background: none;
  }

.pagination a:active{
  text-decoration: none;
  background: url('<%= asset_path('small_button.png') %>') left -23px no-repeat;
  _background: url('<%= asset_path('small_button.gif') %>') left -23px no-repeat;
  }
.pagination a:active:after{
  content: "";
  position: absolute;
