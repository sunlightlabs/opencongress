.cf:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.cf:after {
    clear: both;
}

/**
 * For IE 6/7 only
 * Include this rule to trigger hasLayout and contain floats.
 */
.cf {
    zoom: 1;
}

/* ========================================= */
/* site chrome tweaks from homepage redesign */
/* ========================================= */

/* Link color */
.bootstrap-enabled a { color: #448ccb;}

/* Logo Position fix */
a.logo { background: url('<%= asset_path('oc-logo.png') %>') no-repeat 0px 6px; }
a.logo:hover { background-position: 0px -100px; }

/* Nav width fix */
#nav { width: 1002px; }

/* Socialite */

#header .share-buttons {
  position: relative;
  top: 16px;
  left: 23px;
}

/* Register & Login Buttons */

#register-btn.btn.large.btn-primary {
  position: relative;
  left: 18px;
  width: 98px;
  height: 18px;
  padding: 5px;
  margin: 10px 10px 0 0;
  border: 1px solid #d0d0d0;
  border-radius: 3px;
  background: #50a4ed;
  font-family: "franklin-gothic-urw", Helvetica, Arial, sans-serif;
  font-weight: 400;
  font-size: 12px;
  text-shadow: none;
}

#register-btn.btn.large.btn-primary:before {
  content: '';
  position: absolute;
  top: 2px;
  left: 2px;
  width: 102px;
  height: 22px;
  border: 1px dotted #78b8f0;
}

#register-btn.btn.large.btn-primary:hover { background: #4b9ade; }
#register-btn.btn.large.btn-primary:before:hover { border: 1px dotted #80c3ff; }

#login-btn.btn.large.dropdown-toggle {
  position: relative;
  left: 20px;
  width: 98px;
  height: 18px;
  padding: 5px;
  margin: 10px 0 0 0;
  border: 1px solid #d0d0d0;
  border-radius: 3px;
  background: #666;
  font-family: "franklin-gothic-urw", Helvetica, Arial, sans-serif;
  font-weight: 400;
  font-size: 12px;
  text-shadow: none;
  color: #fff;
}

#login-btn.btn.large.dropdown-toggle:before {
  content: '';
  position: absolute;
  top: 2px;
  left: 2px;
  width: 102px;
  height: 22px;
  border: 1px dotted #888;
}

#login-btn.btn.large.dropdown-toggle:hover {
  background: #555;
}

/* Logged in state */

#user_account .logged-in {
  position: relative;
  top: 12px;
  left: 10px;
  font-family: "franklin-gothic-urw", Helvetica, Arial, sans-serif;
}

#user_account .logged-in span {
  font-size: 15px;
}

#user_account #username {
  font-weight: 400;
}

#user_account .btn {
  position: relative;
  top: -2px;
  height: auto;
  padding: 5px;
  border: 1px solid #d0d0d0;
  border-radius: 3px;
  background: #666;
  font-family: "franklin-gothic-urw", Helvetica, Arial, sans-serif;
  font-weight: 400;
  font-size: 12px;
  text-shadow: none;
  color: #fff;
}
#user_account .btn:hover {background: #555;}

#user_account #logout-btn {
  width: 48px;
}

/* Temporary Donate Button */
#user_account .btn.btn-info {
  background: #7db4c5;
  border: 1px solid #abd2de;
  text-shadow: 0 0 1px rgba (0, 0, 0, 0.1);
  padding: 5px 10px;
}
#user_account .btn.btn-info:hover {
  background: #5299ad;
}

/* Login Dropdown */

.bootstrap-enabled #user_account .dropdown-menu {
  width: 234px;
  left: 18px;
  margin-top: 15px;
  border-radius: 3px;
}

.bootstrap-enabled #user_account .dropdown-menu:before {
  left: 174px;
}

.bootstrap-enabled #user_account .dropdown-menu:after {
  left: 175px;
}

.bootstrap-enabled .login form {
  width: 200px;
  padding: 5px 17px;
  margin: auto;
  border-bottom: 1px solid #eee;
}

.bootstrap-enabled .login legend {
  margin-bottom: 5px;
  border-bottom: none;
  font-weight: normal;
  font-size: 14px;
  color: #444;
}

.bootstrap-enabled #user_account .dropdown-menu input{
  width: 180px;
  padding: 6px 8px;
  margin-bottom: 15px;
  border: 1px solid #d0d0d0;
  border-radius: 3px;
  font-family: "franklin-gothic-urw", Helvetica, Arial, sans-serif;
  font-size: 14px;
  color: #444;
}

.bootstrap-enabled #user_account .dropdown-menu input[type=password]{ letter-spacing: -1px;}

.bootstrap-enabled #user_account .dropdown-menu input::-webkit-input-placeholder { color: #bbb; letter-spacing: 0;}
.bootstrap-enabled #user_account .dropdown-menu input:-moz-placeholder { color: #bbb; letter-spacing: 0; opacity: 1;}
.bootstrap-enabled #user_account .dropdown-menu input::-moz-placeholder { color: #bbb; letter-spacing: 0; opacity: 1;}
.bootstrap-enabled #user_account .dropdown-menu input:-ms-input-placeholder { color: #bbb; letter-spacing: 0;}
.bootstrap-enabled #user_account .dropdown-menu input::input-placeholder { color: #bbb; letter-spacing: 0;}

.bootstrap-enabled .login form button {
  display: inline-block;
  width: 195px;
  padding: 5px;
  margin-bottom: 10px;
  border: 0;
  border-radius: 3px;
  background: #50a4ed;
  -webkit-box-shadow: none;
  box-shadow: none;
  font-family: "franklin-gothic-urw", Helvetica, Arial, sans-serif;
  opacity: 1;
}

.bootstrap-enabled .login form button:hover, .bootstrap-enabled .login form button:focus {
  background: #4b9ade;
}

#user_account .dropdown-menu a {
  display: block;
  margin-bottom: 10px;
  text-align: center;
}

#user_account .dropdown-menu form a {
  color: #bbb;
}

#user_account .dropdown-menu .fb_iframe_widget {
  margin: 20px 45px 15px;
}

/* Search */
/*#search { position: relative;}*/

.bootstrap-enabled #search .search {
  left: 15px;
  margin-top: 8px;
}

.bootstrap-enabled .search.input-append .add-on,
.bootstrap-enabled .search.input-prepend .add-on {
  height: auto;
  opacity: .5;
}

.bootstrap-enabled .search.input-append .add-on:hover,
.bootstrap-enabled .search.input-prepend .add-on:active {
  opacity: .9;
}

.bootstrap-enabled .search input[type=text] {
  padding-right: 26px;
  width: 165px;
}

.bootstrap-enabled .search input[type=text]::-webkit-input-placeholder { color: #bbb;}
.bootstrap-enabled .search input[type=text]:-moz-placeholder { color: #bbb; opacity: 1;}
.bootstrap-enabled .search input[type=text]::-moz-placeholder { color: #bbb; opacity: 1;}
.bootstrap-enabled .search input[type=text]:-ms-input-placeholder { color: #bbb;}
.bootstrap-enabled .search input[type=text]::input-placeholder { color: #bbb;}

/* Search Options Button & Dropdown */

.bootstrap-enabled .search button.inset {
  position: relative;
  right: 26px;
  border: none;
  background: transparent;
  box-shadow: none;
  z-index: 9;
}

.bootstrap-enabled .search .dropdown-menu {
  width: 225px;
  padding: 0;
  margin-top: 15px;
  text-align: left;
}

#search a.add-on.dropdown-toggle { border: 1px solid #aaa; }

#search a.add-on.dropdown-toggle:hover {
  -moz-box-shadow: none;
  -webkit-box-shadow: none;
  box-shadow: none;
}

#search a.add-on.dropdown-toggle:active {
  border: 1px solid #ccc;
  background: -webkit-gradient(linear, left top, left bottom, from(rgba(0,0,0,.02)), to(rgba(0,0,0,.1)));
  background: linear-gradient(to bottom, rgba(0,0,0,.02), rgba(0,0,0,.1));
  -moz-box-shadow: none;
  -webkit-box-shadow: none;
  box-shadow: none;
}

.bootstrap-enabled .search.dropdown.arrowed .dropdown-menu:before { left: 5px; }

.bootstrap-enabled .search.dropdown.arrowed .dropdown-menu:after {
  left: 6px;
  border-bottom-color: #eee;
}

.bootstrap-enabled .search .dropdown-menu ul { margin-left: 0; }

.bootstrap-enabled .border-left {
  padding-left: 20px;
  border-left: 2px groove rgba(240, 240, 240, 0.3);
}

.bootstrap-enabled .search .dropdown-menu #sessions,
.bootstrap-enabled .search .dropdown-menu #search_in {
  float: none;
  width: auto;
  min-width: 180px;
  min-height: 0;
  border-left: none;
}

#search_in p, #sessions p{
  padding: 10px 20px;
  font-size: 13px;
}

#sessions label, #search_in label {
  padding: 1px 10px;
  font-size: 13px;
}

#search_in p{
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
}

.bootstrap-enabled .search #autocomplete-spin {
  margin-left: 0;
  margin-top: 0;
  right: 27px;
  opacity: 1;
  border: 0;
  top: 1px;
  width: 16px;
  padding: 4px;
  background: #fff;
}

.bootstrap-enabled #search-autocomplete-choices {
  left: 30px;
  width: 197px !important;
  max-height: 385px;
  margin: 0;
  margin-top: -1px;
  padding: 0;
  border: 1px solid #ccc;
  background: #fff;
  font-size: 10px;
  line-height: 18px;
  overflow: scroll;
  overflow-y: scroll;
  overflow-x: hidden;
}

.bootstrap-enabled #search-autocomplete-choices h2 {
  padding: 0 10px;
  color: #666;
}

